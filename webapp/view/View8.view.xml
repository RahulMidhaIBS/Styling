<mvc:View controllerName="CSS_Assignment.CSS_Assignment.controller.View8" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
						<Table id = "idTable" items = "{path : 'empdetails>/Employee'}">
							<columns>
								<Column hAlign="Center" minScreenWidth="Tablet" demandPopin="true" width="15%">
									<Text text = "Name"></Text>
								</Column>
								<Column hAlign="Center" minScreenWidth="Tablet" demandPopin="true" width="15%">
									<Text text = "Age"></Text>
								</Column>
								<Column hAlign="Center" minScreenWidth="Tablet" demandPopin="true" width="15%">
									<Text text = "Organisation"></Text>
								</Column>
								<Column hAlign="Center" minScreenWidth="Tablet" demandPopin="true" width="15%">
									<Text text = "Domain"></Text>
								</Column>
							</columns>
							
							<items>
								<ColumnListItem>
									<cells>
										<Text wrapping = "true" text = "{empdetails>name}"></Text>
									</cells>
									<cells>
										<Text wrapping = "true" text = "{empdetails>age}"></Text>
									</cells>
									<cells>
										<Text wrapping = "true" text = "{empdetails>org}"></Text>
									</cells>
									<cells>
										<Text wrapping = "true" text = "{empdetails>dom}"></Text>
									</cells>
								</ColumnListItem>
							</items>
						</Table>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>